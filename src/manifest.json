{
  "name": "OddsJam Enhancer",
  "version": "0.0.1.0",
  "description": "Enhances the OddsJam website.",
  "icons": {
    // "48": "icon48.png",
    // "128": "icon128.png"
  },
  "manifest_version": 3,
  "content_scripts": [{
     "matches": ["https://oddsjam.com/*"],
     "css": ["content-script.css"],
     "js": [
       "common.js",
       "content-script.js"
     ]
  }],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
